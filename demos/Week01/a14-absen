exit 0

## BASH IGNORES AFTER EXIT

w
export PROCPS_USERLEN=16
w
w | gawk '$5 ~ /s$/ {print}'
w | gawk '$5 ~ /s$/ {print $1}' | sort -u
w | gawk '$5 ~ /s$/ {print $1, strftime("%y%m%d-%H%M%S", systime())}'
w | gawk '$5 ~ /s$/ {print $1, strftime("%y%m%d-%H%M%S", systime())}' | sort -u

